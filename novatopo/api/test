query{
  allBooking{
    name
    email
    total
    date
    participant
    isStaff
    activity{
      name
      address
      business {
        name
      }
    }
  }
}

mutation{
  createBooking(
    name: "felix maillet",
    email: "felixmaillet@yahoo.fr",
    participant: 2,
    date: "2020-01-12T18:00:00+00:00",
    isStaff: false,
    activity: "Karting",
  ){
    id
    name
    email
    participant
    date
    total
    isStaff
    activity
  }
}
